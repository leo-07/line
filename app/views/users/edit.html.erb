<div class="header_fixed">
       <div class="header">
          <div class="clear_box">
            <%= link_to '戻る',friends_path,class:"header_button btn btn-primary"%>
          </div>
             <div class="float_left">
               <h1>登録情報</h1>
             </div>
       </div>
</div>
<div class="body form_sheet">
  <div class="col-lg-12">
    <%= form_for current_user do |f| %>
      <div class="form-group">
        <div class="col-xs-12 padding_10">
          <h2>名前</h2>
          <%= f.text_field :nickname, placeholder: "Nickname", class: "form-control" %>
        </div>
        <div class="col-xs-12 padding_10">
            <div class="">
            </div>
            <h2>プロフィール画像</h2>
            <%= f.file_field :avatar%>
        </div>
      </div>
      <div class="form-group padding_10">
        <div class="clear_box">
        </div>
        <%= f.submit "送信", class: "btn btn-primary withripple" %>
      </div>
    <% end %>
  </div>
</div>